{
  "name": "theprojectarchive-root",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm --prefix next-app install",
    "dev": "npm --prefix next-app run dev",
    "build": "npm --prefix next-app run build && rm -rf _static && mkdir -p _static && cp -R next-app/out/. _static",
    "export": "npm --prefix next-app run build && rm -rf _static && mkdir -p _static && cp -R next-app/out/. _static",
    "start": "NODE_ENV=production node server.js",
    "test": "npm --prefix next-app run test",
    "lint": "npm --prefix next-app run lint",
    "format": "npm --prefix next-app run format",
    "check": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "dependencies": {
    "mime-types": "^2.1.35",
    "ws": "^8.17.0"
  }
}
